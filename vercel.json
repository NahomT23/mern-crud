{
    "version": 2,
    "builds": [
      {
        "src": "vehicle-crud/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1"
      }
    ]
  }
  